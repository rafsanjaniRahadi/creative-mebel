<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- mebel.pembuatan tree view -->
    <record id="mebel_pembuatan_view_tree" model="ir.ui.view">
        <field name="name">mebel pembuatan</field>
        <field name="model">mebel.pembuatan</field>
        <field name="arch" type="xml">
            <tree>
                <!-- Add your fields here -->
                <field name="name_mebel"/>
                <field name="tanggal_pesan"/>
                <field name="tanggal_jadi"/>
                <field name="total"/>
            </tree>
        </field>
    </record>

    <!-- mebel.pembuatan form view -->
    <record id="mebel_pembuatan_view_form" model="ir.ui.view">
        <field name="name">Form pembuatan</field>
        <field name="model">mebel.pembuatan</field>
        <field name="arch" type="xml">
            <form string="Pembuatan Detail">
                <sheet>
                    <group>
                        <field name="name_mebel"/>
                        <field name="tanggal_pesan"/>
                        <field name="tanggal_jadi"/>
                        <field name="total"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <!-- mebel_pembuatan_action action window -->
    <record id="mebel_pembuatan_action" model="ir.actions.act_window">
        <field name="name">pembuatan Panggung</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">mebel.pembuatan</field>
        <field name="view_mode">tree,form</field>
    </record>
    <!-- This Menu Item must have a parent and an action -->
    <menuitem id="mebel_pembuatan_menu_act" name="pembuatan Mebel" parent="mebel_pembuatan_menu_categ" action="mebel_pembuatan_action" sequence="20"/>

</odoo>